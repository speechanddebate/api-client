{
    "name": "api-client",
    "version": "1.0.0",
    "type": "module",
    "description": "NSDA API Client",
    "main": "index.ts",
    "engines": {
        "node": ">= 20.13.1"
    },
    "scripts": {
        "build": "tsc",
        "lint": "eslint src/**/*.ts* --max-warnings 0",
        "format": "prettier src/ --write",
        "format-check": "prettier src/ --check",
        "test": "vitest watch",
        "test-silent": "vitest --silent",
        "test-cover": "vitest --coverage",
        "test-ci": "vitest --run",
        "prepare": "husky"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/speechanddebate/api-client.git"
    },
    "author": "Aaron Hardy <aaron.hardy@speechanddebate.org> (https://speechanddebate.org)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/speechanddebate/api-client/issues"
    },
    "homepage": "https://github.com/speechanddebate/api-client#readme",
    "dependencies": {
        "@speechanddebate/nsda-js-utils": "git+https://github.com/speechanddebate/nsda-js-utils.git#v2",
        "js-cookie": "^3.0.5"
    },
    "devDependencies": {
        "@speechanddebate/eslint-config-nsda": "git+https://github.com/speechanddebate/eslint-config-nsda.git#v2",
        "@speechanddebate/types": "git+https://github.com/speechanddebate/types.git",
        "@types/chai": "^4.3.12",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/parser": "^7.6.0",
        "chai": "^5.1.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.9.0",
        "eslint-plugin-vitest": "^0.3.22",
        "globals": "^14.0.0",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "msw": "^2.3.0",
        "prettier": "3.2.5",
        "typescript": "^5.2.2",
        "typescript-eslint": "^7.1.1",
        "vite": "^5.1.4",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^1.3.1"
    }
}
